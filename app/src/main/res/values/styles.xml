<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="workpoint">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#000000</item>
        <item name="fontFamily">@font/roboto_light</item>
    </style>

    <style name="welcome">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#000000</item>
        <item name="fontFamily">@font/open_sans_light</item>
    </style>

    <style name="some_id">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#991B1F3B</item>
        <item name="fontFamily">@font/open_sans_light</item>
    </style>
    <style name="sign_in">
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">#991B1F3B</item>
        <item name="fontFamily">@font/open_sans_semi_bold</item>
    </style>

    <style name="question">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#991B1F3B</item>
        <item name="fontFamily">@font/opensans_regular</item>
    </style>

    <style name="welcome_reg">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#000000</item>
        <item name="fontFamily">@font/open_sans_light</item>
    </style>

    <style name="send_text">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#000000</item>
        <item name="fontFamily">@font/opensans_regular</item>
    </style>

    <style name="text_radio">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="buisnes">
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">#99000000</item>
    </style>

    <style name="errors_empty">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#CC0000</item>
        <item name="fontFamily">@font/open_sans_light</item>
    </style>

    <style name="errors_exception">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#CC0000</item>
        <item name="fontFamily">@font/open_sans_light</item>
    </style>

    <style name="MyRadioButton" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@android:color/holo_green_light</item>
        <item name="colorControlActivated">@android:color/holo_orange_dark</item>
    </style>

    <style name="text_geo">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#CC1B1F3B</item>
        <item name="fontFamily">@font/opensans_regular</item>
    </style>

    <style name="text_cities">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#000000</item>
        <item name="fontFamily">@font/opensans_regular</item>
    </style>

    <style name="ed_hint_search">
        <item name="android:textSize">8sp</item>
        <item name="fontFamily">@font/open_sans_light</item>
        <item name="android:textColor">#661B1F3B</item>
    </style>

    <style name="text_filters">
        <item name="android:textSize">10sp</item>
        <item name="fontFamily">@font/open_sans_semi_bold</item>
        <item name="android:textColor">#991B1F3B</item>
    </style>

    <style name="text_search_cow">
        <item name="android:textSize">8sp</item>
        <item name="fontFamily">@font/open_sans_semi_bold</item>
        <item name="android:textColor">#991B1F3B</item>
    </style>

    <style name="text_show_carte">
        <item name="android:textSize">8sp</item>
        <item name="fontFamily">@font/open_sans_semi_bold</item>
    </style>

    <style name="text_h2_place_card">
        <item name="android:textSize">13sp</item>
        <item name="fontFamily">@font/open_sans_semi_bold</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="rating_bar" parent="Widget.AppCompat.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_ratingbar</item>
        <item name="android:minHeight">15dip</item>
        <item name="android:maxHeight">30dip</item>
    </style>

    <style name="rating_bar_review_other" parent="Widget.AppCompat.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_ratingbar_review_other</item>
        <item name="android:minHeight">10dip</item>
        <item name="android:maxHeight">15dip</item>
    </style>

    <style name="RoundScuare">
        <item name="cornerSize">10%</item>
    </style>

    <style name="settings_profile">
        <item name="android:textSize">7sp</item>
        <item name="android:textColor">#661B1F3B</item>
        <item name="fontFamily">@font/opensans_regular</item>
    </style>
</resources>